@model IEnumerable<MDB.Models.Login>

@{
    ViewBag.Title = "LoginsJournal";
}

<h2>Journal des connexions</h2>

<div id="LoginsListContainer">
    @{ Html.RenderAction("GetLoginsList", new { forceRefresh = true }); }
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

    <script defer>
        PartialRefresh.setEndSessionAction("@Url.Action("End","Session")");
        let pr = new PartialRefresh("@Url.Action("GetLoginsList")", "LoginsListContainer", 5);
    </script>
}

